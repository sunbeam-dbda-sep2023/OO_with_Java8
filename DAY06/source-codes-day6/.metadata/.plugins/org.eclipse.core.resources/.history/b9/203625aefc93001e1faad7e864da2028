public class Day6_6 {

	public static void main(String[] args)
	{
		String s1 = "sunbeam";
		String s2 = "Sunbeam";
		String s3 = "sunbeam";
		String s4 = s3;
		String s5 ="java";
		String s6 = new String("java");
		String s7=new String("dbda");
		String s8=new String("sunbeam");
		String s9 = new String("Sunbeam").intern();
		System.out.println("s5 == s6 "+(s5==s6));
		System.out.println("S5 = "+s5+ " Hashcode = "+s5.hashCode());
		System.out.println("S6 = "+s6+ " Hashcode = "+s6.hashCode());
		
		
	}
}
