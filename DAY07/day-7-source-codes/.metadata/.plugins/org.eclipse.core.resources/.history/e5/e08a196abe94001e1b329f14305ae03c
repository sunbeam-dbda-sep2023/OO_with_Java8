class Person
{
	String name;
	int age;
	
	Person()
	{
		
	}
	Person(String name,int age)
	{
		this.name=name;
		this.age=age;
	}
	@Override
	public String toString() {
		return "Person [name=" + this.name + ", age=" + this.age + "]";
	}	
} // end of Person class



class Employee extends Person
{
	int empid;
	int salary;
	
	
}


