package p1;

public class Circle extends BoundedShape
{
	int radius;
	Circle()
	{
		
	}
	Circle(int radius)
	{
		this.radius=radius;
	}
	@Override
	public String toString() {
		return "Circle [radius=" + radius + "]"+super.toString();
	}
	@Override
	void area() {
		// TODO Auto-generated method stub
		
	}
	
	

}
