// join example

public class Day15_8 extends Thread
{
	
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			System.out.println(i+"\t");
			try
			{
				Thread.sleep(1000);
			}
			catch(Exception e)
			{
				System.out.println(e);
			}
		}
	}
	public static void main(String[] args) 
	{
		Day15_8 t1 = new Day15_8();

	}

}
