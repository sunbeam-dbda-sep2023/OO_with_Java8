
public class Demo 
{
	void m1() // non static method
	{
		System.out.println("Demo class M1 method is called");
	}
	
	static void add()
	{
		System.out.println("Demo Addition method called = "+(90+50));
	}
	
	static void sub()
	{
		System.out.println("Demo Subtraction method called = "+(10-8));
	}
}
