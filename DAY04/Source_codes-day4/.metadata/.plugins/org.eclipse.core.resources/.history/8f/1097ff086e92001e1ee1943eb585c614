// final reference

class Circle
{
	int radius;
	
	Circle()
	{
		
	}
	Circle(int radius)
	{
		this.radius = radius;
	}
	
	void disp()
	{
		System.out.println("Radius of a Circle = "+this.radius);
	}
}

// c1 is non final reference
public class Day4_12 {

	public static void main(String[] args)
	{
		Circle c1 = new Circle(5);
		c1.disp();
		c1 = new Circle(6);
		c1.disp();

	}

}
