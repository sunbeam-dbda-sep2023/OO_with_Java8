public class Day9_10 {

	public static void main(String[] args) 
	{
		ArrayList<Integer> al = new ArrayList<Integer>();
		al.add(25);
		al.add(35);
		al.add(20);
		al.add(10);
		System.out.println("AL = "+al);
		al.add(2,99);
		System.out.println("AL = "+al);
	}
}




