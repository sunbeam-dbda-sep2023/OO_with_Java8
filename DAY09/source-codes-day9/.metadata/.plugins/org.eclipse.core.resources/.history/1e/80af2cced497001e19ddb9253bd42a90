public class Day9_2 {

	public static void main(String[] args)
	{
		System.out.println("Core java DBDA");
		try
		{
			Thread.sleep(5000);
		}
		catch(InterruptedException e)
		{
			System.out.println(e);
		}
			
			System.out.println("topic is checked vs unchecked");
	}

}
