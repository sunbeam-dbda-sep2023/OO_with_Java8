class Student
{
	private int rn;
	private int age;
	String name;
	int marks;
	double per;
	
	
	void disp()
	{
		System.out.println("Roll no = "+this.rn+" Age = "+this.age);
		System.out.println("Name = "+this.name+" Marks = "+this.marks);
		System.out.println("Percentage = "+this.per);
	}
}
