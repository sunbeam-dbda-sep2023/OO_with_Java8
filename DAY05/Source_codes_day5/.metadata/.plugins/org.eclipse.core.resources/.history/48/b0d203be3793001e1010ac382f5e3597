public class Day5_14 {

	public static void main(String[] args) 
	{
		Integer i[]=new Integer[] {1,2,3,4,5};
		int arr[]=new int[] {11,22,33,44};
		
		for(Integer element : i)
			System.out.print(element+"\t");
		
	}
}
