public class Day5_4 
{
	public static void main(String[] args) 
	{
		System.out.println(ceil(45.3));
		System.out.println(floor(152.9));
		System.out.println(max(9, 8));
		System.out.println(min(25, 15));
		System.out.println(sqrt(25));
		System.out.println(pow(7, 2));

	}

}
