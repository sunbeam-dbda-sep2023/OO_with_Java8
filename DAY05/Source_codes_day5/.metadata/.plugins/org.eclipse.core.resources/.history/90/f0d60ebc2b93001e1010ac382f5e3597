import java.util.Scanner;

// take input from user and display array elements 

public class Day5_6 {

	public static void main(String[] args) 
	{
		int a[]= new int[5];
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the array elements :");
		for(int i=0;i<5;i++)
			a[i] = sc.nextInt();
		

	}

}
