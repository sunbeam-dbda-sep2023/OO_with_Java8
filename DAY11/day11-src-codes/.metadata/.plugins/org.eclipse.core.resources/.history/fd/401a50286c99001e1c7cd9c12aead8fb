import java.util.HashMap;

//HashMap
// unordered
// unsorted
// duplicate key is not allowed , if we do so, it will overwrite its value
// duplicate value is allowed

public class Day11_4 {

	public static void main(String[] args) 
	{
		HashMap<Integer,String> hm = new HashMap<Integer,String>();
		hm.put(20,"st1");
		hm.put(30,"st2");
		hm.put(40,"st3");
		hm.put(10,"st4");
		hm.put(4,"st5");
		System.out.println("Hash Map = "+hm); 
		hm.put(4,"st6");
		System.out.println("Hash Map = "+hm); 
		hm.put(50,"st6");
		System.out.println("Hash Map = "+hm); 
		hm.put(null,"st6");
	}

}
