//TreeSet
// Unordered
// Sorted
// no duplicates are allowed

public class Day11_2 {

	public static void main(String[] args)
	{
		TreeSet<Integer> ts = new TreeSet<Integer>();
		ts.add(20);
		ts.add(10);
		ts.add(15);
		ts.add(25);
		ts.add(55);
		System.out.println("Contents of hash set = "+ts);
		ts.add(25);
		
		System.out.println("Contents of hash set = "+ts);
		//ts.add(null);
		//System.out.println("Contents of hash set = "+ts);
		//ts.add(null);
		//System.out.println("Contents of hash set = "+ts);
	}

}


