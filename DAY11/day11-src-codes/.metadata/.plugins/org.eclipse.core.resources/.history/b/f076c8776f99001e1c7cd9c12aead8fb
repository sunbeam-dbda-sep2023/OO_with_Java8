import java.util.Collection;
import java.util.HashMap;
import java.util.List;
import java.util.Set;

// HashMap

public class Day11_5 {

	public static void main(String[] args)
	{
		HashMap<Integer,String> hm = new HashMap<Integer,String>();
		hm.put(10, "emp1");
		hm.put(20, "emp2");
		hm.put(30, "emp3");
		hm.put(40, "emp4");
		System.out.println("Contents of HashMap = "+hm);
		System.out.println(hm.containsKey(10)); // true
		System.out.println(hm.containsKey(55)); //false
		System.out.println(hm.containsValue("emp2")); // true
		System.out.println(hm.containsValue("emp25")); //false 
		
		System.out.println("Keyset values are");
		Set<Integer> s = hm.keySet();
		System.out.println(s);
		
		System.out.println("Fetching the values ");
		
		Collection c = hm.values();
		System.out.println(c);
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
