//TreeMap
//unordered
//sorted
//duplicate key is not allowed , if we do so, it will overwrite its value
//duplicate value is allowed
//null key is not allowed (otherwise it will throw NPE)
//value can be assigned null to many keys



public class Day11_4 {

	public static void main(String[] args) 
	{
		TreeMap<Integer,String> tm = new TreeMap<Integer,String>();
		tm.put(20,"st1");
		tm.put(30,"st2");
		tm.put(40,"st3");
		tm.put(10,"st4");
		tm.put(4,"st5");
		System.out.println("Tree Map = "+tm); 
		tm.put(4,"st6");
		System.out.println("Hash Map = "+tm); 
		tm.put(50,"st6");
		System.out.println("Hash Map = "+tm); 
		//tm.put(null,"st7"); // NPE
		//System.out.println("Hash Map = "+tm); 
		tm.put(99,null);
		System.out.println("Hash Map = "+tm); 
		tm.put(88,null);
		System.out.println("Hash Map = "+tm); 
	}

}